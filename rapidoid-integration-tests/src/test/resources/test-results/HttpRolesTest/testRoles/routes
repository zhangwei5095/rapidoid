Route DELETE /del [zone main] roles [administrator, eraser, manager] : json(Ctrl#del())
Route GET /a [zone main] roles [aa, bb] : json(() -> ...)
Route GET /hi [zone main] roles [administrator, manager, moderator] : json(Ctrl#hi())
Route GET /ok [zone main] roles [] : json(() -> ...)
Route HEAD /h [zone main] roles [logged_in] : json(Ctrl#h())
Route OPTIONS /opt [zone main] roles [logged_in, a, b, anybody] : json(Ctrl#opt())
Route PATCH /p3 [zone main] roles [patcher] : json(Ctrl#p3())
Route POST /p1 [zone main] roles [administrator] : json(Ctrl#p1())
Route PUT /p2 [zone main] roles [moderator] : json(Ctrl#p2())